#include <stdio.h>

int main(void) {
    int t;
    scanf("%d",&t);
    while(t--){
        int n,x,y,i,count=0;
        scanf("%d %d %d",&n,&x,&y);
        int a[n];
        for(i=0;i<n;i++) {
    scanf("%d",&a[i]);
        
        if(a[i]<=x  && a[i] % y == 0){
            count++;
          
        }
        }
      printf ("%d \n", count);
    }


	return 0;
}
